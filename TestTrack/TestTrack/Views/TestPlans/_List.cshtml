@model TestTrack.ViewModels.TestPlansListVM

@foreach (var item in Model.Values)
{
    <div class="row">
        <div class="col-md-1">
            <div class="green-square"></div>
        </div>
        <div class="col-md-8">
            <p>
                <strong>@Html.ActionLink(item.Title, "Index", "TestRunsOnTestPlan", new { id = item.TestPlanID }, null)</strong><br />
                @item.Description
            </p>
            <p><small>Created on @item.CreatedOn.</small></p>
        </div>
        <div class="col-md-3">
            
        </div>
    </div>
}
